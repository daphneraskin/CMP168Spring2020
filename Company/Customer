package com.myorganization.customers;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.List;

import com.myorganization.employees.Employee;

public class Customer {
	public static void main(String[] args) {
		Person p1 = new Person("Mike", 28);
		Person p2 = new Person("Rachel", 25);
		Person p3 = new Person("Betty", 40);
		Person p4 = new Person("Tom", 18);
		
		List<Person> people = new ArrayList<>(Arrays.asList(p1, p2, p3, p4));
		System.out.println(people);
		Collections.sort(people);
		System.out.println(people);
		
	}
}
