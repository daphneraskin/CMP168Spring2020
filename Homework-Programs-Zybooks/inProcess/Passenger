
public class Passenger {
	String name;
	int birthYear;
	double weight;
	char gender;
	int numCarryOn;
	
	public Passenger(){
		name = "";
		birthYear = 1900;
		weight = 0.0;
		gender = 'u';
	}
	public Passenger(java.lang.String name, int birthYear, double weight, char gender, int numCarryOn) {
		this.name = name;
		this.birthYear = birthYear;
		this.weight = weight;
		this.gender = gender;
	}
	
	public void calculateAge(int currentYear) {
		int age = currentYear - birthYear;
	}
	
	public void gainWeight() {
		weight += 1;
	}
	
	public void gainWeight(double weight) {
		this.weight += weight;
	}
	
	public int getBirthYear() {
		return birthYear;
	}
	
	public char getGender() {
		return gender;
	}
	
	public String getName() {
		return name;
	}
	
	public void setNumCarryOn(int numCarryOn) {
		this.numCarryOn = numCarryOn;
	}
	
	public int getNumCarryOn() {
		return numCarryOn;
	}
	
	public double getWeight() {
		return weight;
	}
	
	public boolean isFemale() {
		boolean see;
		if (gender == 'f') {
			see = true;
		}
		else {
			see = false;
		}
		return see;
	}
	
	public boolean isMale() {
		boolean see;
		if (gender == 'm') {
			see = true;
		}
		else {
			see = false;
		}
		return see;
	}
	
	public void loseWeight() {
		weight -= 1;
	}
	
	public void loseWeight(double weight) {
		weight -= weight;
	}
	
	public void printDetails() {
		System.out.printf(name + birthYear + weight + gender);
	}
	
	public void setBirthYear(int birthYear) {
		this.birthYear = birthYear;
	}
	
	public void setGender(char gender) {
		this.gender = gender;
	}
	
	public void setName(java.lang.String name) {
		this.name = name;
	}
	
	public void setWeight(double weight) {
		this.weight = weight;
	}
	
	
}
